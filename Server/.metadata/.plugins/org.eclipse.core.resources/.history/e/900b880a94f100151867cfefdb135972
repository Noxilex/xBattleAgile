package mainPackage;

public class Coord {

	private int x;
	private int y;
	
	public Coord(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public Coord() {}
	
	/**
	 * @return the Coord x
	 */
	public int x() {
		return this.x;
	}
	
	/**
	 * @return the Coord y
	 */
	public int y() {
		return this.y;
	}
	
	/**
	 * Set the coord x
	 * @param x
	 */
	public void setX(int x) {
		this.x = x;
	}
	
	/**
	 * Set the coord y
	 * @param y
	 */
	public void setY(int y) {
		this.y = y;
	}
	
	/**
	 * 
	 */
	public String toString() {
		return "("+x+";"+y+")";
	}
	
}
